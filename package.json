{
  "name": "todolangeditor",
  "version": "1.0.0",
  "description": "TodoLang editor",
  "scripts": {
    "dev": "npm start",
    "start": "webpack-dev-server --hot --open",
    "antlr4ts": "antlr4ts  ./FormulaParser.g4 ./FormulaLexer.g4 -o ./src/ANTLR",
    "dumi:dev": "dumi dev"
  },
  "author": "Open",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@toy-box/formula": "^1.0.13",
    "@toy-box/meta-schema": "^2.0.3",
    "antlr4ts": "^0.5.0-alpha.4",
    "dumi": "^1.1.32",
    "father-build": "^1.20.5-0",
    "monaco-editor-core": "^0.18.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "css-loader": "^3.3.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
